input#item1[type="checkbox"] {
            width: 20px;
            height: 20px;
            opacity: 0;
        }
        
label.item_noname {
            position: relative;
            left: 0;
            top: 8px;
            width: 18px;
            height: 18px;
            border-radius: 50%;
            background-color: white;
            border: 1px solid white;
}
        
input#item1:checked+label.item_noname {
            background-color: white;
            border: 1px solid white;
}
        
input#item1:checked+label.item_noname::after {
            position: absolute;
            content: "";
            width: 5px;
            height: 10px;
            top: 3px;
            left: 6px;
            border: 2px solid #25a7f5;
            border-top: none;
            border-left: none;
            transform: rotate(45deg)
}
