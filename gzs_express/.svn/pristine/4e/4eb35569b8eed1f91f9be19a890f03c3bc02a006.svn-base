.black_overlay{ 
             display: none; 
             position: absolute; 
             top: 0%; 
             left: 0%; 
             width: 100%; 
             height: 100%; 
             background-color: black; 
             z-index:999; 
             -moz-opacity: 0.6; 
             opacity:.60; 
             filter: alpha(opacity=60); 
}
