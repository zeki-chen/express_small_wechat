@media screen and (max-width: 1296px) {
    .car_part2{
    	padding: 38px 0.02rem 0 0.02rem;
    }
    .car_img{
    	width: 135px;
    }
    .car_detail_div div{
    	width: 100px;
    }
    .car_detail_div1 span{
    	margin: 0 .065rem;
    }
}    
@media screen and (min-width: 947px) and (max-width: 1164px) {
    .car_no{
    	display: none !important;
    }
}
@media screen and (max-width: 591px) {
    
    .multi_choose2{
    	top: 20px;
    }

}
@media screen and (max-width: 1178px) {
    .register_div, .login_div{
        width: 41%;
    }
    .car_img{
    	left: -0.02rem;
    }

}
@media screen and (max-width: 914px) {
    .register_div, .login_div{
        width: 48%;
    }
    
    .order_btn{
    	width: 100px;
    }
    
}

@media screen and (max-width: 834px) {
    .multi_choose1{
        position: absolute;
    }
    .car_detail_div span{
        margin: 0 .05rem;
    }
    .register_div, .login_div{
        width: 56%;
    }
   
}
@media screen and (max-width: 794px) {
    .goto_page{
        top: -12px;
    }
    .register_div, .login_div{
        width: 65%;
    }
    
}
@media screen and (max-width: 678px) {
    
    .car_list{
    	text-align: center;
    }
    .car_part22{
    	display: none;
    }
    .car_part2,.car_part1{
    	width: 25%;
    }
    .page_div{
    	margin-right: 0rem;
        font-size: 16px;
    }
    .order_btn{
    	width: 70px;
    }
    .register_div, .login_div{
        width: 80%;
    }
    .VR_img{
        left: 28px;
    }
    .car_div{
        padding: 10px 10px 10px 10px;
    }
}



