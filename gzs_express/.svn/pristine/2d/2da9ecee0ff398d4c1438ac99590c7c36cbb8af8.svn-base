        </div>
        <div class="">
            <if condition="$user[user_id] eq ''">
                <button type="button" class="start_btn"><a href="javascript:void(0)" onclick="show_loginHtml()">开始赚钱</a></button>
                <else/>
            <button type="button" class="start_btn"><a href="{:U('Individual/individual')}">开始赚钱</a></button>
       </if> </div>
    </div>
</div>


<div class="special_price_div">
    <h2>今日特价</h2>
    <h3>IT'S TIME TO DRIVE</h3>

    <div class="container car_price_div">
        <div class="row">
            <foreach name="list2" item="vo">
                <div class="col-sm-6 col-lg-4">
                    <div class="parent" onclick=window.open("{:U('Detail/detail',array('car_id'=>$vo['car_id']))}");>
                        <div class="background"></div>
                        <div class="price_div img">
                            <span class="today_price">￥{$vo.price}/今日</span><span
                                class="car_detail">{$vo.gear_name}</span><span
                                class="car_detail">{$vo.car_number}</span><span class="car_detail"><eq
                                name="vo['intro']['self']" value="1">自动应答<else/>无自动应答</eq></span><img
                                src="__PUBLIC__/Index/images/xing.png" class="xing_img"><img src="{$vo.pic}"
                                                                                             class="car_img img-responsive">
                        </div>
                    </div>
                </div>
            </foreach>
