                        <else/>
                        <a href="{:U('Individual/individual')}">{$user.user_name}</a>
                <li><a href="{:U('Index/quit')}" target="_parent">退出</a></li>
                </eq></li>
                <li><a href="#"><img data-original="__PUBLIC__/Index/images/location.png"
                                     class="location_img lazy"><span class="location_a"></span></a></li>
            </ul>

        </div>

    </nav>
