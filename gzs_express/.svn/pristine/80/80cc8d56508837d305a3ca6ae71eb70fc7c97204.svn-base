@keyframes fade-in {  
    0% {opacity: 0;top: 100%;}/*初始状态 透明度为0*/  
    40% {opacity: 0;top: 80%;}/*过渡状态 透明度为0*/  
    100% {opacity: 1;top: 45%;}/*结束状态 透明度为1*/  
}  
@-webkit-keyframes fade-in {/*针对webkit内核*/  
    0% {opacity: 0;top: 100%;}/*初始状态 透明度为0*/  
    40% {opacity: 0;top: 80%;}/*过渡状态 透明度为0*/  
    100% {opacity: 1;top: 45%;}/*结束状态 透明度为1*/  
}  
.wrapper {    
    /*play 3s linear infinite;*/
    animation: fade-in;/*动画名称*/  
    animation-duration: 5s;/*动画持续时间*/  
    -webkit-animation:fade-in 5s;/*针对webkit内核*/  
} 




